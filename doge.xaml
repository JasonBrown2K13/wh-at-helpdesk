                <TextBlock x:Name="textBlock_Copy7" HorizontalAlignment="Left" Margin="54,264,0,0" TextWrapping="Wrap" Text="WOW!" VerticalAlignment="Top" FontFamily="Comic Sans MS" Foreground="yellow" FontSize="68"/>
